#include <stdio.h>

int main() {
    int i,n;
    int fact=1; //for factorial
    printf("Enter the number: ");
    scanf("%d",&n);
    if(n<0)
    {
        printf("Enter valid number!");
    }
    else
    {
        for(i=1;i<=n;i++)
        {
            fact=fact*i;
        }
        printf("The factorial of %d is %d",n,fact);
    }

    return 0;
}

